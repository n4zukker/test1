		var prev_id 	= '';
		var current 	= '';
		var _counter 	= 0;
		var _seconds 	= 0;
		var base_url 	= base_url();
		
		/* -- OVERLAY VARIABLES ---------------------- */
		var size 	= 0.85;
		var w_w		= $(window).width();
		var w_h		= $(window).height();
		var m_w 	= Math.round(w_w * size);
		var m_h 	= Math.round(w_h * size);
		var o_w 	= m_w;
		
		/* -- NOTIFICATION VARIABLES ------------------ */
		var n 		= 1420362259; 
		var u 		= 60*1000;
		var title 	= document.title;
		var message = false;

		/* -- SET THE NOTIFICATION COOKIE ------------- */ 
		if ($.cookie('snw-notify')) {
			 n = $.cookie('snw-notify');
		} else {
			$.cookie('snw-notify', n, {expires:1, path:'/'})	
		}
		
		function updateNotification() {
			
			$.getJSON("/includes/cache/recent-posts.cache", function(data) {
				
				var t = 0;
				
				$.each(data, function(k, v) {
					
					if (k > n) {
						t += 1;
					} else {
						//return false;	
					}
				
				});
				
				if (t > 0) {
					
					var message = (t == '1') ? 'New article just published' : t+' new articles published.';
					
					if (t > 30) {
						message = '30+ new articles published';	
					}
					
					$("#notification").html('<p id="notification-message"><a href="/index.php?view='+n+'" data-event="notification">'+message+' Click To View</a></p>');
					
					if (!$("#notification").is(":visible")) {
						$("#notification").slideDown(250);
					}
					
					document.title = '('+t+') ' + title;
				}
			
			});

		}
		
		var notificationId = setInterval(updateNotification, u);
		
		function base_url() {
			
			var base_url = window.location.pathname;
			var base_arr = base_url.split("/"); 
			
			base_url = '/' + base_arr[1] + '/' + base_arr[2] + '/' + base_arr[3] + '/';
			
			return base_url;
		}
		
		function openMenu() {

			_seconds++;

			if(_seconds == 2) { 
				showMenu();
				clearInterval(_counter);
			}
		
		}
		
		function showMenu() {
							
			var ts = current;
			var id = '#'+ ts.attr('rel');
			var li = ts.parent();
			var pr = $('.current-menu');
			var pe = li.prev().find("a");
			var pc = pr.find("a");
			var pw = pr.prev().find("a");
			
			pw.css('background-position', 'right 0px');
			pc.css('background-position', 'right 0px');
			
			ts.css('background-position', 'right -36px');
			pe.css('background-position', 'right -72px');
							
			pr.removeClass('current-menu');
			li.addClass('current-menu');
			
			if (!show) {
				$("#menu-arrow").show();
				$(id).slideDown(500);
				show = true;
				prev_id = id;
			} else {
				if (id != prev_id) {
					$(prev_id).hide();
					$(id).show();
					prev_id = id;
				}
				$(".menu-sub-entries").hide();
				$(id).find("#sub-menu-main").show();	
			}
			
			var pos 	= li.position();
			var left 	= (id == '#menu-front-page') 	? pos.left 		 	: (pos.left - 12)
			var width	= (id == '#menu-front-page') 	? (li.width() - 14) : (li.width() - 2);
				width	= (id == '#menu-celebrities') 	? (li.width() + 12) : width;
				
			$("#menu-arrow").css("width", width + "px");
			$("#menu-arrow").css("left",  left  + "px");

		}
		
		$(document).ready(function() {
			
			// CHECK FOR ANALYTICS EVENT DETECTION ------------ //
			$("a[data-event]").click(function(e) {
				var ev = $(this).attr('data-event');
				ga('send', 'event', 'linksource', ev);
				return true;
			});
			
			/* ----------------------------------------------- */
			/* -- MAILING LIST OVERLAY ----------------------- */
			if (!$.cookie('snw-mailing-overlay') && !$.cookie('snw-mailing-signup')) {
				$.cookie('snw-mailing-overlay', '1', {expires:7, path:'/'});
				$("#email-overlay").delay(3000).fadeIn(250);
			} 

			$("#email-overlay").click(function(e){
				$("#email-overlay").fadeOut(250);
			});
			$("#email-overlay-close").click(function(e){
				$("#email-overlay").fadeOut(250);
			});
			$("#email-overlay-wrapper").click(function(e){
				e.stopPropagation();
			});
			
			// -- EMAIL SUBMISSION --------------------------- //
			$("#ff-subscribe-overlay").click(function(e) {
				
				e.preventDefault();

				var email = $("#ff-email-overlay").val();	
				
				if (email && email != 'Your Email Address') {
					$("#email-form-overlay").html('<div id="ff-email-confirm-overlay">' + email + '</div><button type="button" name="confirm" id="ff-subscribe-confirm-overlay"> Confirm </button>');	
				} else {
					alert("Please fill in your Email");	
				}
			
			});

			$("#email-form-overlay").on("click", "#ff-subscribe-confirm-overlay", function(e) {

				var email = $("#ff-email-confirm-overlay").html();
				
				$.ajax({
					type: 	"POST",
					url: 	"/includes/ajax/email-newsletter.php",
					data: 	"verify=8902fn48s8gy&email=" + email,
					success: function(msg){
						console.log(msg);
						$("#email-form-overlay").html('Newsletter Signup Completed!');
					}
				});		
				
				e.stopPropagation();
				
			});
			
			// NEW ARTICLE NOTIFICATIONS ----------------------- //
			var notificationId = setInterval(updateNotification, u);
			
			$("#notification").on('click', function(e) {
				
				$("#notification-posts").slideDown(250);

				n = Math.floor(d.getTime() / 1000);
				$.cookie('snw-notification', n, {expires:1, path:'/'})
				
				document.title = title;
				
			});

			// FORCE BLANK WINDOW OPEN ------------------------ //
			$("#image-description a").click(function(e) {
				var attr = $(this).attr('data-event');
				if (typeof attr == typeof undefined || attr == false) {
					$(this).attr('target', '_blank');
				} else {
					var pos = $("a[name=purchase-links]").offset().top;
					$("html,body").animate({scrollTop:pos},500);	
				}
			});
			
			// -- MENU --------------------------------------- //
			$(".menu-category-link").hover(function(e){

				var id = $(this).attr("rel");
				var dv = $(this).parentsUntil('#menu-sub-area');
				
				$(".menu-sub-entries").hide();
				$("#"+id).show();
					
			});
			
			if (screen.width <= 768) {
				
				$('.menu-link').click(function(e) { 
					current = $(this);
					var id 	= '#' + current.attr('rel');
					if (id != prev_id) {
						e.preventDefault();
						showMenu();
					}
				});

			} else {
			
				$(".menu-link").hover(function(){
					
					current = $(this);
					
					if (!show) {
						 _counter = setInterval(openMenu, 250);
					} else {
						showMenu()
					}
		
				}, function() {
					clearInterval(_counter);
				});
				
				$(".video-entry img").hover(function(){
					$(this).parent().parent().find(".video-entry-title a").css("color", "#d2232a");
				}, function() {
					$(this).parent().parent().find(".video-entry-title a").removeAttr("style");
				});
			
			}

			// -- EMAIL SUBMISSION --------------------------- //
			$("#ff-subscribe").click(function(e) {
				
				e.preventDefault();

				var email = $("#ff-email").val();	
				
				if (email && email != 'Your Email Address') {
					$("#email-form").html('<div id="ff-email-confirm">' + email + '</div><button type="button" name="confirm" id="ff-subscribe-confirm">Confirm</button>');	
				} else {
					alert("Please fill in your Email");	
				}
			
			});

			$("#email-form").on("click", "#ff-subscribe-confirm", function(e) {
				
				e.preventDefault();
				
				var email = $("#ff-email-confirm").html();
				
				$.ajax({
					type: 	"POST",
					url: 	"/includes/ajax/email-newsletter.php",
					data: 	"verify=8902fn48s8gy&email=" + email,
					success: function(msg){
						console.log(msg);
						$("#email-form").html('Newsletter Signup Completed!');
					}
				});		
				
			});

			// -- FEATURE VARIABLES ----------------------------- //
			slide_w			= $("#area-gallery").width();
			slide_tot		= $("#gallery-main").children().size();
			list			= $("#area-gallery").attr('rel');

			// -- FORWARD THE IMAGE ----------------------------- //
			if (image_id != '0') {
				
				$("#gallery-slide-1").css('left', '-'+slide_w+'px');
				
				var cur = $("div[rel="+image_id+"]");
				
				cur.css('left', '0px');
				
				slide_cur = $("#gallery-main").children().index(cur) + 1;
				
				updateImage();
				
			} else {
				
				var cur = $("#gallery-slide-1");
				
				updateVideo(cur);	
				
			}
			
			// -- ADD GALLERY TOUCHACTIONS ----------------------- //
			$("#area-gallery").touchSwipe(touchAction);
			
			function touchAction(direction) {
				
				if (direction == 'left') {
					moveSlide('r');	
				} else {
					moveSlide('l');	
				}
				
			}
			
			// -- UPDATE IMAGE COUNTER --------------------------- //
			updateCounter();
			
			// -- ADD GALLERY NAVIGATION ACTIONS ---------------- //
			$("#image-gallery-nav-z").click(function(e){
				e.preventDefault();
				var image_url = $("#gallery-slide-"+slide_cur+ " a").attr('href');
				lightboxOpen(image_url);
			});
			
			$("#image-gallery-nav-l").click(function(e){
				e.preventDefault();
				moveSlide('l');
			});
			
			$("#image-gallery-nav-r").click(function(e){
				e.preventDefault();
				moveSlide('r');
			});
			
			$(".image-zoom").click(function(e){
				e.preventDefault();
				var image_url = $(this).attr('href');
				lightboxOpen(image_url);
			});
			
			$("body").on("click", "#gallery-overlay", function(e){
				e.preventDefault();
				lightboxClose();	
			});
			
			$(".video-play").click(function(e){
				
				e.preventDefault();
				
				var video 	= $(this).attr("data-video");
				var html 	= '<div id="video-container"><iframe class="youtube-player" type="text/html" src="'+video+'" frameborder="0" allowfullscreen></iframe></div>';

				$(this).hide();
				$(this).parent().append(html);
				
				$("#image-gallery-nav-z").hide();	
				
			});
			
			// -- ADD GALLERY ANIMATIONS ------------------- //
			function slideAnimate(slide_num) {
				
				var $slide_s = $("#gallery-slide-"+slide_cur);
				var $slide_e = $("#gallery-slide-"+slide_num);

				$slide_e.css("left", start_p + "px");
				
				$slide_s.animate(
								 {left:end_p}, 
								 {duration: 200, step: function(now, fx) {
										$slide_e.css("left", (start_p + now));
								 	}, complete: function(){
										updatePrev();
										slide_cur 	= slide_num;
										image_id 	= $slide_e.attr('rel');
										updateImage();
										updateURL();
										updateDescription();
										updateStat();
									}
								 });
			}
			
			function slideInit(slide_num) {
				
				slide_obj = $(".gallery-thumb-"+slide_num);
				slide_pre = $(".current-slide");
				
				$(".current-slide").removeClass("current-slide");
				slide_obj.addClass("current-slide");
								
				if (slide_num != slide_cur) {
					slideAnimate(slide_num);
				}
				
			}

			// -- ADD GALLERY SHOW/HIDE NAVIGATION ------------------- //
			function showLinks() {
				
				if (current > 0) {
					$("#image-gallery-nav-l").show();
				} else {
					$("#image-gallery-nav-l").hide();	
				}
				
				if (current < total) {
					$("#image-gallery-nav-r").show();
				} else {
					$("#image-gallery-nav-r").hide();	
				}

			}
			
			// -- ADD MOVEMENT FUNCTIONS --------------------------- //
			function moveSlide(dir) {
				
				if (dir == 'r') {
					
					slide_num 	= (slide_cur == slide_tot) ? 1 : slide_cur + 1;
					start_p 	=  slide_w 
					end_p		= -slide_w;
				
				} else {
					
					if (list == '1' && cycle) {
						slide_num = (slide_cur == 1) ? false : slide_cur - 1;
					} else {
						slide_num = (slide_cur == 1) ? slide_tot : slide_cur - 1;	
					}
					
					start_p 	= -slide_w 
					end_p		= slide_w;
				
				}

				if (!$("#gallery-slide-"+slide_cur).is(':animated') && slide_num){
					slide_next = slide_num;
					slideInit(slide_num);
				} 
			
			}
			
			// -- UPDATE DESCRIPTION DIV --------------------------- //
			function updateDescription() {
				
				if (image_id && snwlist[image_id]) {
					$("#image-description").html(snwlist[image_id]);
				} else {
					$("#image-description").html(snwlist[1]);					
				}
				
			}
			
			// -- UPDATE MEDIA DIV --------------------------- //
			function updateVideo(slide) {
				
				var element = slide.find("a");
				var video 	= element.attr("data-video");
				
				if (video) {
					
					$("#image-gallery-nav-z").hide();	
						
					var html = '<div id="video-container"><iframe class="youtube-player" type="text/html" src="'+video+'" frameborder="0" allowfullscreen></iframe></div>';
				
					// HIDE THE IMAGE LINK ELEMENT /////////////
					element.hide();
				
					// APPEND THE HTML TO SLIDE ////////////////
					element.parent().append(html);
				
				} else {
					
					$("#image-gallery-nav-z").show();	
					
				}
				
			}

			// -- RELATED GALLERY SWAP --------------------------- //
			$(".related-link").click(function(e){
				e.preventDefault();
			});
			
			var related_current = 'content-popular-area';
			
			$(".related-link").mouseover(function(){
				
				var view = $(this).attr("rel");
				
				if (view != related_current) {
									
					$("#"+related_current).hide();
					$("#"+view).show();

					$("#content-related ul").removeClass(related_current);
					$("#content-related ul").addClass(view);
					
					related_current = view;	
				}

			});
			
			$('#form-purchase').submit(formValidate);
			
			/* -- FACEBOOK RENDER FOR HIDDEN DIV ------- */	
			var subscribe	= document.getElementsByClassName("social-media-icon-h")[0];
			var fb_wrap		= document.getElementsByClassName("fb-wrap")[0]; 
    		var rendered 	= false;

			window.fbAsyncInit = function () {
    			subscribe.onmouseover = function () {
        			if (!rendered) {
            			FB.XFBML.parse(fb_wrap);
           				rendered = true;
        			}
    			};
			};
			
			/* -- SOCIAL MEDIA ROLLOVERS --------------- */
			$(".social-media-icon-h").hover(function(e) {
				var $em = $(this).find(".social-media-badge");
				$em.css("z-index", '101');
				$em.hide();
				$em.slideDown(250);
			}, function(e) {
				$(this).find(".social-media-badge").slideUp(250);
			});
			
		});
		
		function lightboxOpen(url) {
			
			var img = new Image();
			
			img.onload = function() {

				var i_w = img.width;
				var i_h = img.height;
					
				var l_w = (m_h / i_h) * i_w;
				var l_h = m_h;
				
				l_w = Math.round(l_w);
				
				if (l_w > o_w) {
					l_w = o_w;
					l_h = (l_w / i_w) * i_h;
					l_h = Math.round(l_h);
				}

				m_t = (w_h - l_h) / 2;
				
				$("#gallery-overlay-image").attr('src', url);
				$("#gallery-overlay-image").attr("style", "margin-top:"+m_t+"px; width:"+l_w+"px; height:"+l_h+"px;");
				
				$('#gallery-overlay').fadeIn('fast');
				
			};
			
			img.src = url;
			
		}
		
		function lightboxClose() {
			$('#gallery-overlay').fadeOut('fast');
			$('#gallery-overlay').remove('img');	
		}

		
		function updatePrev() {
			
			var cur_video = $("#gallery-slide-"+slide_cur).find("#video-container");
			
			if (cur_video) {
				$("#video-container").remove();
